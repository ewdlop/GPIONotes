   "source": [
    "from gpiozero import LED \n",
    "from time import sleep \n",
    "\n",
    "red = LED(17) \n",
    " \n",
    "while True: \n",
    "    red.on()    #turn led on \n",
    "    sleep(1)    #delay for 1 second \n",
    "    red.off()   #turn led off \n",
    "    sleep(1)    #delay for 1 second"
   ]
  }
